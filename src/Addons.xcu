     <value>standardbar</value>
    </prop>
    <prop oor:name="MergePoint">
     <value>.uno:ExportDirectToPDF</value>
    </prop>
    <prop oor:name="MergeCommand">
     <value>AddAfter</value>
    </prop>
    <prop oor:name="MergeFallback">
