          <value>com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon:ExportCommand</value>
        </prop>
        <node oor:name="UserDefinedImages">
          <prop oor:name="ImageSmallURL">
            <value>%origin%/images/daisy-hi-16.gif</value>
          </prop>
          <prop oor:name="ImageBigURL">
            <value>%origin%/images/daisy-hi-26.gif</value>
          </prop>

        </node>
      </node>
      <node oor:name="com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon.exportfullcommand.images" oor:op="replace">
        <prop oor:name="URL" oor:type="xs:string">
          <value>com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon:ExportFullCommand</value>
        </prop>
        <node oor:name="UserDefinedImages">
          <prop oor:name="ImageSmallURL">
