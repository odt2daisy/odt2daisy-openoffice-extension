    </prop>
    <prop oor:name="MergeContext">
     <value>com.sun.star.text.TextDocument</value>
    </prop>
    <node oor:name="ToolBarItems">
     <node oor:name="B1" oor:op="replace">
      <prop oor:name="URL">
       <value>com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon:ExportCommand</value>
      </prop>
      <prop oor:name="Title">
       <value xml:lang="en">Export Directly as DAISY XML</value>
       <value xml:lang="fr">Export Direct au format DAISY XML</value>
