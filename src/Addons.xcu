       <value xml:lang="es">Exportar directamente en formato DAISY XML</value>
       <value xml:lang="zh">直接地输出为 DAISY XML</value>
      </prop>
     </node>
     <node oor:name="B2" oor:op="replace">
      <prop oor:name="URL">
       <value>com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon:ExportFullCommand</value>
      </prop>
      <prop oor:name="Title">
       <value xml:lang="en">Export Directly as Full DAISY</value>
       <value xml:lang="fr">Export Direct au format Full DAISY</value>
       <value xml:lang="it">Esporta direttamente come Full DAISY</value>
       <value xml:lang="ja">DAISYとして直接エクスポート</value>
       <value xml:lang="sl">Izvozi neposredno kot Full DAISY</value>
       <value xml:lang="nl">Direct als DAISY exporteren</value>
       <value xml:lang="gl">Exportar directamente como Full DAISY</value>
       <value xml:lang="ru">Экспортировать непосредственно как Full DAISY</value>
       <value xml:lang="sv">Exportera direkt som Full DAISY</value>
       <value xml:lang="de">Direkt als Full DAISY Exportieren</value>
       <value xml:lang="ca">Exporteu directament com a Full DAISY</value>
       <value xml:lang="es">Exportar directamente en formato Full DAISY</value>
       <value xml:lang="zh">直接地输出为 Full DAISY</value>
      </prop>
     </node>
    </node>
   </node>
  </node>
 </node>


    <node oor:name="Images">
