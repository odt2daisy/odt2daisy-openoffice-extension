<oor:component-data xmlns:oor="http://openoffice.org/2001/registry" xmlns:xs="http://www.w3.org/2001/XMLSchema" oor:name="Addons" oor:package="org.openoffice.Office">
  <node oor:name="AddonUI">

  <node oor:name="OfficeMenuBarMerging">
   <node oor:name="com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon" oor:op="replace">
    <node oor:name="S1" oor:op="replace">
     <prop oor:name="MergePoint">
      <value>.uno:PickList\.uno:ExportToPDF</value>
     </prop>
     <prop oor:name="MergeCommand">
      <value>AddAfter</value>
     </prop>
     <prop oor:name="MergeFallback">
      <value>AddPath</value>
     </prop>
     <prop oor:name="MergeContext">
      <value>com.sun.star.text.TextDocument</value>
     </prop>
     <node oor:name="MenuItems">
      <node oor:name="M1" oor:op="replace">
       <prop oor:name="URL">
          <value>com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon:ExportCommand</value>
       </prop>
       <prop oor:name="Title">          
         <value xml:lang="en">Export as DAISY X~ML...</value><!-- d, x are already in use by default; e is too common -->
         <value xml:lang="fr">Exporter au format DAISY X~ML...</value><!-- d, f, x, y, e, l, a are already in use by default -->
         <value xml:lang="it">~Esporta come DAISY XML...</value>
         <value xml:lang="ja">~DAISY XMLとしてエクスポート...</value>
         <value xml:lang="sl">~Izvozi kot DAISY XML...</value>
         <value xml:lang="nl">Exporteren als DAISY-X~ML...</value><!-- d, f, x are already in use by default -->
         <value xml:lang="gl">~Exportar como DAISY XML...</value>
         <value xml:lang="ru">~Экспортировать как DAISY XML...</value>
         <value xml:lang="sv">~Exportera som DAISY XML...</value>
         <value xml:lang="de">Exportieren als DAISY-X~ML ...</value><!-- d, f, x, e, l, f, u are already in use by default -->
         <value xml:lang="ca">~Exporteu com a DAISY XML...</value>
         <value xml:lang="es">Exportar en formato DAISY X~ML...</value><!-- d, f, x, y, e are already in use by default -->
         <value xml:lang="zh">输出为 DAISY XML (~M)...</value><!-- d, f, x, a are already in use by default -->
        </prop>
      </node>
      <node oor:name="M2" oor:op="replace">
       <prop oor:name="Title">          
         <value xml:lang="en">Export as Full DAIS~Y...</value><!-- d, x are already in use by default; e is too common -->
         <value xml:lang="fr">Exporter au format Full DAIS~Y...</value><!-- @todo d, f, x, y, e, l, a are already in use by default -->
         <value xml:lang="it">~Esporta come Full DAISY...</value>
         <value xml:lang="ja">~Full DAISYとしてエクスポート...</value>
         <value xml:lang="sl">~Izvozi kot Full DAISY...</value>
         <value xml:lang="nl">Exporteren als Full DAIS~Y...</value><!-- d, f, x are already in use by default -->
         <value xml:lang="gl">~Exportar como Full DAISY...</value>          
         <value xml:lang="ru">~Экспортировать как Full DAISY...</value>
         <value xml:lang="sv">~Exportera som Full DAISY...</value>
         <value xml:lang="de">Exportieren als Full DAIS~Y...</value><!-- d, f, x, e, l, f, u are already in use by default -->
         <value xml:lang="ca">~Exporteu com a Full DAISY...</value>
         <value xml:lang="es">Exportar en formato Full DAIS~Y...</value><!-- d, f, x, y, e are already in use by default -->
         <value xml:lang="zh">输出为 Full DAISY (~Y)...</value><!-- d, f, x, a are already in use by default -->
        </prop>
        <prop oor:name="URL">
          <value>com.versusoft.packages.ooo.odt2daisy.addon.odt2daisyaddon:ExportFullCommand</value>
         </prop>
      </node>
     </node>
    </node>
   </node>
  </node>
  
<node oor:name="OfficeToolbarMerging">
